! from the template file /lusr/share/udb/pub/dotfiles/Xdefaults
!
! The following defaults change the behavior of clients using the
! X libraries.

XTerm*font: 	        9x15
XTerm*Background:	blue4
!XTerm*borderWidth:	2
XTerm*BorderColor:	yellow
XTerm*cursorColor:	white
XTerm*visualBell:       on
XTerm*ttyModes:		intr^cerase^hkill^ususp^z
XTerm*Foreground:	moccasin
XTerm*reverseWrap:	on
XTerm*pointerColor:	yellow

xclock*foreground:	turquoise
