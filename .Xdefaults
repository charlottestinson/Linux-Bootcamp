xclock*border:		pink
xclock*highlight:	gold
xclock*hands:		yellow
xclock*mode:		analog
xclock*font: 		9x15
xclock*padding:		10

Emacs*Font:		9x15

xman*Text.border:	Blue
xman*foreground:	yellow
xman*background:	Black
xman*borderColor:	Blue
